SOLUTION ===>

#include <iostream>
#include <vector>
using namespace std;

void can_partition_chocolates(int n, vector<int>& chocolates) {
    int total = 0;
    for (int i = 0; i < n; ++i)
        total += chocolates[i];

    // If total chocolates are odd, can't split equally
    if (total % 2 != 0) {
        cout << "NO";
        return;
    }

    int target = total / 2;
    vector<bool> dp(target + 1, false);
    dp[0] = true;

    // Subset sum DP approach
    for (int i = 0; i < n; ++i) {
        for (int j = target; j >= chocolates[i]; --j) {
            dp[j] = dp[j] || dp[j - chocolates[i]];
        }
    }

    if (dp[target])
        cout << "YES";
    else
        cout << "NO";
}

int main() {
    int n;
    cin >> n;
    vector<int> chocolates(n);
    for (int i = 0; i < n; ++i) {
        cin >> chocolates[i];
    }
    can_partition_chocolates(n, chocolates);
    return 0;
}
