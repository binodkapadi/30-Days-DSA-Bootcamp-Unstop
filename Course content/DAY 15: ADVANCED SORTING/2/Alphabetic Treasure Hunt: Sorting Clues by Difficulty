SOLUTION ===>>

#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

void user_logic(int n, string &s) {
    sort(s.begin(), s.end(), [](char a, char b) {
        int diff_a = (a - 'a' + 1) % 5;
        int diff_b = (b - 'a' + 1) % 5;
        if (diff_a != diff_b)
            return diff_a < diff_b;        
        return a > b;                      
    });
}

int main() {
    int n;
    string s;
    
    cin >> n >> s;
    
    user_logic(n, s);
    
    cout << s << endl;
    
    return 0;
}
