#include <iostream>
#include <vector>
#include <cmath>
using namespace std;

static bool isPrimeNumber(int x) {
    if (x <= 1) return false;
    if (x <= 3) return true;
    if (x % 2 == 0) return false;
    int r = static_cast<int>(sqrt(x));
    for (int d = 3; d <= r; d += 2) {
        if (x % d == 0) return false;
    }
    return true;
}

static bool isPerfectSquareNumber(int x) {
    if (x < 0) return false;
    int r = static_cast<int>(sqrt(x));
    return r * r == x;
}

void transform_matrix(vector<vector<int>>& matrix, int n, int& row_count, int& prime_count) {
    // Keep original values for square detection
    vector<vector<int>> orig = matrix;

    // Step 1: Replace primes by 0 (in matrix) and remember prime positions if needed
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            if (isPrimeNumber(orig[i][j])) {
                matrix[i][j] = 0;
            } else {
                // keep original (not prime) value for now
                matrix[i][j] = orig[i][j];
            }
        }
    }

    // Step 2: Find rows/cols to be marked -1 due to perfect squares (use orig)
    vector<bool> markRow(n, false);
    vector<bool> markCol(n, false);
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            if (isPerfectSquareNumber(orig[i][j])) {
                markRow[i] = true;
                markCol[j] = true;
            }
        }
    }

    // Step 3: Apply -1 to marked rows and columns. -1 has priority over 0
    for (int i = 0; i < n; ++i) {
        if (markRow[i]) {
            for (int j = 0; j < n; ++j) {
                matrix[i][j] = -1;
            }
        }
    }
    for (int j = 0; j < n; ++j) {
        if (markCol[j]) {
            for (int i = 0; i < n; ++i) {
                matrix[i][j] = -1;
            }
        }
    }

    // Step 4: Count rows fully -1 and count zeros in final matrix
    row_count = 0;
    prime_count = 0; // number of zeros remaining in final matrix

    for (int i = 0; i < n; ++i) {
        bool allMinusOne = true;
        for (int j = 0; j < n; ++j) {
            if (matrix[i][j] != -1) {
                allMinusOne = false;
            }
            if (matrix[i][j] == 0) {
                ++prime_count;
            }
        }
        if (allMinusOne) ++row_count;
    }
}

int main() {
    int n;
    cin >> n;
    vector<vector<int>> matrix(n, vector<int>(n));
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            cin >> matrix[i][j];
        }
    }

    int row_count = 0;
    int prime_count = 0;
    transform_matrix(matrix, n, row_count, prime_count);

    cout << row_count << " " << prime_count << endl;
    for (const auto& row : matrix) {
        for (int val : row) {
            cout << val << " ";
        }
        cout << endl;
    }

    return 0;
}
