SOLUTION ==>>

#include <iostream>
using namespace std;

int calculateX(int N) {
    if (N == 0) return 5;
    if (N == 1) return 3;

    long long prev2 = 5; // F[0]
    long long prev1 = 3; // F[1]
    long long curr = 0;

    for (int i = 2; i <= N; ++i) {
        if (i % 2 == 1) { // odd
            curr = (prev1 + prev2) % 998;
        } else { // even
            curr = (prev1 * prev2) % 999;
        }
        prev2 = prev1;
        prev1 = curr;
    }

    return curr;
}

int main() {
    int N;
    cin >> N;
    int result = calculateX(N);
    cout << result << endl;
    return 0;
}
