SOLUTION ==>>

#include <iostream>
using namespace std;

int get_treasure(int n) {
    if (n == 1) return 1;
    if (n == 2) return 2;

    int a = 1, b = 2, c;
    for (int i = 3; i <= n; ++i) {
        c = a + b; // ways to reach i-th compartment
        a = b;
        b = c;
    }
    return b;
}

int main() {
    int n;
    cin >> n;

    int result = get_treasure(n);
    cout << result << endl;

    return 0;
}
