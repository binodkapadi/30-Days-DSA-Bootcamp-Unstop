SOLUTION ==>>

#include <bits/stdc++.h>
using namespace std;

bool word_break(const string &s, const vector<string> &word_dict) {
    unordered_set<string> dict(word_dict.begin(), word_dict.end());
    int n = s.size();
    vector<bool> dp(n + 1, false);
    dp[0] = true; // empty string is always segmentable

    for (int i = 1; i <= n; ++i) {
        for (int j = max(0, i - 20); j < i; ++j) { // words are at most length 20
            if (dp[j] && dict.find(s.substr(j, i - j)) != dict.end()) {
                dp[i] = true;
                break;
            }
        }
    }

    return dp[n];
}

int main() {
    string s;
    cin >> s;
    int n;
    cin >> n;
    vector<string> word_dict(n);
    for (int i = 0; i < n; ++i) cin >> word_dict[i];

    bool result = word_break(s, word_dict);
    cout << (result ? "true" : "false") << endl;
    return 0;
}
