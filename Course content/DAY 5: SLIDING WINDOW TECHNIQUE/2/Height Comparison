#include <iostream>
#include <vector>
#include <algorithm>
#include <deque>
using namespace std;

vector<int> find_tallest(int N, int K, vector<int> height) {
    vector<int> result;
    deque<int> dq; // Will store indices of potential maximums

    for (int i = 0; i < N; i++) {
        // 1. Remove indices that are out of the current window (i - K)
        if (!dq.empty() && dq.front() == i - K) {
            dq.pop_front();
        }

        // 2. Remove indices from the back if the current height is greater
        // (because the smaller elements will never be the max again)
        while (!dq.empty() && height[dq.back()] <= height[i]) {
            dq.pop_back();
        }

        // 3. Add the current index to the deque
        dq.push_back(i);

        // 4. The front of the deque is the index of the maximum element for the current window
        // We start recording results once we have processed at least K elements (index K-1)
        if (i >= K - 1) {
            result.push_back(height[dq.front()]);
        }
    }
    return result;
}

int main() {
    int N, K;
    cin >> N >> K;
    vector<int> height(N);
    for (int i = 0; i < N; i++) {
        cin >> height[i];
    }
    vector<int> result = find_tallest(N, K, height);
    for (int i = 0; i < result.size(); i++) {
        cout << result[i] << " ";
    }
    cout << endl;
    return 0;
}
