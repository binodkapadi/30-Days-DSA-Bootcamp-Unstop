#include <iostream>
#include <string>
#include <cctype>
#include <cmath>

bool isPrime(int x) {
    if (x < 2) return false;
    for (int i = 2; i * i <= x; i++) {
        if (x % i == 0) return false;
    }
    return true;
}

void check_qualification(const std::string &word) {
     int vowels = 0, consonants = 0;

    for (char ch : word) {
        ch = std::tolower(ch);
        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
            vowels++;
        } else if (ch >= 'a' && ch <= 'z') {
            consonants++;
        }
    }

    // Rule checks
    if (vowels < 2 || consonants == 0) {
        std::cout << "Disqualify";
        return;
    }

    int root = std::sqrt(consonants);
    if (root * root == consonants && isPrime(root)) {
        std::cout << "Qualify";
    } else {
        std::cout << "Disqualify";
    }
}

int main() {
    std::string word;
    std::getline(std::cin, word);
    
    check_qualification(word);
    return 0;
}
