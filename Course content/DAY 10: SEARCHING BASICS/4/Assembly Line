SOLUTION ===>>

#include <iostream>
#include <vector>

void find_student_positions(int n, std::vector<int>& heights, int k, std::vector<int>& queries, std::vector<int>& results) {
     for (int qi = 0; qi < k; qi++) {
        int H = queries[qi];
        int left = 0, right = n;

        // lower_bound logic
        while (left < right) {
            int mid = left + (right - left) / 2;
            if (heights[mid] < H)
                left = mid + 1;
            else
                right = mid;
        }
        results[qi] = left; // insertion or exact position
    }
}

int main() {
    int n, k;
    std::cin >> n;
    std::vector<int> heights(n);
    for (int i = 0; i < n; ++i) {
        std::cin >> heights[i];
    }
    std::cin >> k;
    std::vector<int> queries(k);
    for (int i = 0; i < k; ++i) {
        std::cin >> queries[i];
    }
    std::vector<int> results(k);
    find_student_positions(n, heights, k, queries, results);
    for (int i = 0; i < k; ++i) {
        std::cout << results[i] << std::endl;
    }
    return 0;
}
