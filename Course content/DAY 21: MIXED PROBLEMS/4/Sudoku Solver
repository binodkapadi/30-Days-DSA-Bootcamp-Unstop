SOLUTION ==>>

#include <iostream>
#include <vector>

using namespace std;

bool isValid(vector<vector<char>>& board, int r, int c, char num) {

    for (int i = 0; i < 9; ++i) {
        if (board[r][i] == num) return false;
        if (board[i][c] == num) return false;
    }

    int sr = (r / 3) * 3;
    int sc = (c / 3) * 3;

    for (int i = 0; i < 3; ++i)
        for (int j = 0; j < 3; ++j)
            if (board[sr + i][sc + j] == num)
                return false;

    return true;
}

bool solve(vector<vector<char>>& board) {

    for (int r = 0; r < 9; ++r) {
        for (int c = 0; c < 9; ++c) {

            if (board[r][c] == '.') {

                for (char num = '1'; num <= '9'; ++num) {

                    if (isValid(board, r, c, num)) {

                        board[r][c] = num;

                        if (solve(board))
                            return true;

                        board[r][c] = '.';
                    }
                }

                return false;
            }
        }
    }

    return true;
}

void solve_sudoku(vector<vector<char>>& board) {
    solve(board);
}

int main() {

    vector<vector<char>> board(9, vector<char>(9));

    for (int i = 0; i < 9; ++i)
        for (int j = 0; j < 9; ++j)
            cin >> board[i][j];

    solve_sudoku(board);

    for (const auto& row : board) {
        for (char c : row) {
            cout << c << " ";
        }
        cout << '\n';
    }

    return 0;
}
