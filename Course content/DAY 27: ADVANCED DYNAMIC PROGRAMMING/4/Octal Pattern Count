SOLUTION ==>>

#include <iostream>
#include <vector>
using namespace std;

string user_logic(int N) {
    if (N == 0) return "1"; // Special case

    const int MOD = 1e9 + 7;
    // Map: a,e,i,o,u -> 0,1,2,3,4
    vector<long long> dp(5, 1); // For N=1, each vowel has 1 possibility

    for (int len = 2; len <= N; ++len) {
        vector<long long> new_dp(5, 0);
        new_dp[0] = (dp[1] + dp[4]) % MOD; // a -> e,u
        new_dp[1] = (dp[0] + dp[2]) % MOD; // e -> a,i
        new_dp[2] = (dp[1] + dp[3]) % MOD; // i -> e,o
        new_dp[3] = (dp[2] + dp[4]) % MOD; // o -> i,u
        new_dp[4] = (dp[0] + dp[3]) % MOD; // u -> a,o
        dp = new_dp;
    }

    long long total = 0;
    for (int i = 0; i < 5; ++i) total = (total + dp[i]) % MOD;

    // Convert total to octal
    if (total == 0) return "0";
    string octal = "";
    while (total > 0) {
        octal = char('0' + total % 8) + octal;
        total /= 8;
    }
    return octal;
}

int main() {
    int N;
    cin >> N;
    string result = user_logic(N);
    cout << result << endl;
    return 0;
}
